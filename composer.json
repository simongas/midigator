{
    "name": "webforcehq/order-desk",
    "description": "PHP SDK that consumes the API services from https://www.orderdesk.com/",
    "license": "MIT",
    "authors": [
        {
            "name": "WebforceHQ",
            "email": "fordonez@webforcehq.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^7.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "autoload": {
        "psr-4": {
            "WebforceHQ\\OrderDesk\\": "src/",
